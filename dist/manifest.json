{
  "manifest_version": 2,
  "name": "MTDeck",
  "content_scripts": [
    {
      "js": [
        "./mtdeck.user.js"
      ],
      "matches": [
        "https://tweetdeck.twitter.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "version": "1.2.0",
  "description": "TweetDeckをスマホアプリのように使えるようにする",
  "developer": {
    "name": "Compeito",
    "url": "https://github.com/Compeito/mtdeck"
  }
}